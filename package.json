{
  "name": "rex",
  "displayName": "Rex Lang",
  "description": "Rex Lang Language Support",
  "version": "0.0.3",
  "icon": "icon.png",
  "publisher": "RexLang",
  "activationEvents": [],
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "rex",
        "aliases": [
          "rexLang",
          "rex"
        ],
        "extensions": [
          ".rex",
          ".rex"
        ],
        "configuration": "./language-configuration.json",
        "icon": {
          "dark": "icon.png",
          "light": "icon.png"
        }
      }
    ],
    "grammars": [
      {
        "language": "rex",
        "scopeName": "source.rex",
        "path": "./syntaxes/rex.tmLanguage.json"
      }
    ],
    "snippets": [
      {
        "language": "rex",
        "path": "./snippets/rex.json"
      }
    ]
  },
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "rexLang",
    "Programming",
    "Language"
  ],
  "author": "Fuad Hasan",
  "license": "ISC"
}
